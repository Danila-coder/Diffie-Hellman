        private void button1_Click(object sender, EventArgs e)
        {
            int p = int.Parse(textBox1.Text);
            int q = int.Parse(textBox2.Text);
            int n = p * q;
            int f = (p - 1) * (q - 1);
            textBox3.Text = "\n n = " + n + "\n f = " + f;
        }

        private void button2_Click(object sender, EventArgs e)
        {
            int p = int.Parse(textBox1.Text);
            int q = int.Parse(textBox2.Text);
            int n = p * q;
            int E = int.Parse(textBox4.Text);
            bool prost = true;
            for (int i = 2; i <= E / 2; i++)
            {
                if (E % i == 0)
                {
                    prost = false;
                    break;
                }
            }
            if (prost)
            {
                textBox4.Text = "Открытый ключ " + "\n" + "(" + E + ", " + n + ")";
            }
            else
            {
                textBox4.Text = "Число не простое";
            }
        }

